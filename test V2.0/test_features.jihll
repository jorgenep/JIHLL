# Test HTTP (requires internet)
try:
    resp = httpGet("https://www.google.com")
    if len(resp) > 0:
        print "[PASS] HTTP Get (Size: " + len(resp) + ")"
    .
catch e:
    print "[WARN] HTTP failed (No internet?): " + e
.

print "--- 6. CONCURRENCY ---"
fun worker id:
    sleep(100)
    print "[PASS] Worker " + id + " finished"
.
spawn worker(1)
spawn worker(2)
sleep(200) # Wait for workers