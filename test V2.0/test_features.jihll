print "--- 2. FUNCTIONS & RECURSION ---"
fun fib n:
    if n <= 1: n else: fib(n-1) + fib(n-2).
.
if fib(10) == 55.0:
    print "[PASS] Recursion (Fib 10)"
.

print "--- 3. OOP (CLASSES) ---"
class User:
    fun init name:
        this.name = name
    .
    fun greet:
        "Hello " + this.name
    .
.

u = User("Jill")
if u.greet() == "Hello Jill":
    print "[PASS] Class Methods & Properties"
.

